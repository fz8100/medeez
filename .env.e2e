# E2E Testing Environment Configuration for Medeez v2
# HIPAA-compliant end-to-end testing environment

NODE_ENV=test
E2E_BASE_URL=http://localhost:3000
E2E_API_URL=http://localhost:3001

# Test Authentication
E2E_TEST_USER_EMAIL=test@example.com
E2E_TEST_USER_PASSWORD=TestPassword123!
E2E_TEST_ADMIN_EMAIL=admin@example.com
E2E_TEST_ADMIN_PASSWORD=AdminPassword123!

# Test Clinic Data
E2E_TEST_CLINIC_ID=test-clinic-e2e
E2E_TEST_CLINIC_NAME=E2E Test Medical Clinic

# Browser Configuration
E2E_HEADLESS=true
E2E_SLOW_MO=0
E2E_TIMEOUT=30000
E2E_VIEWPORT_WIDTH=1280
E2E_VIEWPORT_HEIGHT=720

# Test Data
E2E_ENABLE_TEST_DATA=true
E2E_CLEANUP_TEST_DATA=true

# Security Testing
E2E_ENABLE_SECURITY_TESTS=true
E2E_ENABLE_ACCESSIBILITY_TESTS=true
E2E_ENABLE_PERFORMANCE_TESTS=true

# Reporting
E2E_GENERATE_SCREENSHOTS=true
E2E_GENERATE_VIDEOS=false
E2E_GENERATE_TRACES=true

# HIPAA Compliance
E2E_HIPAA_MODE=true
E2E_NO_PHI_IN_LOGS=true
E2E_SECURE_ARTIFACTS=true

# Feature Flags
E2E_SKIP_SLOW_TESTS=false
E2E_PARALLEL_WORKERS=1